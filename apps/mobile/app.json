{
  "expo": {
    "name": "EveryPlayer",
    "slug": "every-player",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "dark",
    "splash": {
      "backgroundColor": "#0A0E8F"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.everyplayer.app"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      },
      "package": "com.everyplayer.app"
    },
    "plugins": [
      "expo-router"
    ],
    "scheme": "every-player"
  }
}
